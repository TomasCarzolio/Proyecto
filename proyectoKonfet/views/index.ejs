	<!-- Header -->
	<%- include('./partials/header') %>
	<!-- /Header -->
	
	<!-- Banner -->
	<div class="banner">
		<img src="/images/bannerConcierto" alt="Banner">
	</div>
	<!-- /Banner -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Novedades</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<% for(let i = 0; i < productos.length; i++) { %> 
				<div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="/products/<%= productos[i].id %> ">
							<figure class="product-box_image">
								<img src="<%= productos[i].foto %>" alt="">
							</figure>
							<article class="product-box_data">
								<h2> <%= productos[i].artista %> </h2>
								<p> <%= productos[i].descripcion %> </p>
								<p> <%= comentarios[i].nombreUsuario %> </p>
								<p> <%= comentarios[i].comentarios %> </p>
							</article>
						</a>
					</section>
				</div>
				<% } %>
		</div>
	</div>
	<!-- /MÃ¡s comentados -->


	<!-- footer -->
	<%- include('./partials/footer') %>
	<!-- /footer -->